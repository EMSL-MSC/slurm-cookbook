<%- @config.to_hash.keys.sort.each do |key| %>
    <%- if not @config[key].nil? %>
<%= key %>=<%= @config[key] %>
    <% end %>
<%- end %>
<%- @nodes.each do |line| %>
<%= line %>
<%- end %>
<%- @partitions.each do |line| %>
<%= line %>
<%- end %>
